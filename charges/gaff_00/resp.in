
&files
  -o    gaff_00/resp.out
&end

&fitq
  ipolq  gaff_00/GenConformers/Conf1/grid_output.vacu  gaff_00/GenConformers/Conf1/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf2/grid_output.vacu  gaff_00/GenConformers/Conf2/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf3/grid_output.vacu  gaff_00/GenConformers/Conf3/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf4/grid_output.vacu  gaff_00/GenConformers/Conf4/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf5/grid_output.vacu  gaff_00/GenConformers/Conf5/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf6/grid_output.vacu  gaff_00/GenConformers/Conf6/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf7/grid_output.vacu  gaff_00/GenConformers/Conf7/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf8/grid_output.vacu  gaff_00/GenConformers/Conf8/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf9/grid_output.vacu  gaff_00/GenConformers/Conf9/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf10/grid_output.vacu  gaff_00/GenConformers/Conf10/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf11/grid_output.vacu  gaff_00/GenConformers/Conf11/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf12/grid_output.vacu  gaff_00/GenConformers/Conf12/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf13/grid_output.vacu  gaff_00/GenConformers/Conf13/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf14/grid_output.vacu  gaff_00/GenConformers/Conf14/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf15/grid_output.vacu  gaff_00/GenConformers/Conf15/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf16/grid_output.vacu  gaff_00/GenConformers/Conf16/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf17/grid_output.vacu  gaff_00/GenConformers/Conf17/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf18/grid_output.vacu  gaff_00/GenConformers/Conf18/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf19/grid_output.vacu  gaff_00/GenConformers/Conf19/grid_output.solv   gaff_00/mon_V.top   1.0
  ipolq  gaff_00/GenConformers/Conf20/grid_output.vacu  gaff_00/GenConformers/Conf20/grid_output.solv   gaff_00/mon_V.top   1.0

  % The minimum proximity of any two points to be included in the fit
  flim      0.39

  % The number of fitting points to select from each electrostatic potential grid 
  nfpt      3750

  % Calculation settings
  maxmem    6GB
  verbose   1

  % Constraints on equivalent atoms
  equalq    ':MON & @H,H1,H2'
  equalq    ':MON & @H3,H4'
  equalq    ':MON & @H5,H6,H7'

  % Restrain charges on buried atoms
  minq      ':MON & @C7'
  minq      ':MON & @C8'
  minq      ':MON & @C3'

  % Force constant by which to restrain charges
  minqwt    1.0e-2

  % Sum of all charges (total charge restraint)
  qtot      0.0

  %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
  %%% The following solvent probe parameters are for SPC/Eb %%%
  %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
  % Lennard-Jones sigma (Å) of solvent probe
  psig      3.1657
  % Lennard-Jones epsilon (kcal/mol) of solvent probe
  peps      0.1553
  % The probe arm; points on the electrostatic potential grid that 
  % would be inaccessible to the solvent probe may still be included 
  % in the fit if they are within the probe arm’s reach
  parm      1.01

  % Maximum Lennard-Jones energy (kcal/mol) of solvent probe at which
  % a point will qualify for inclusion in the fit
  pnrg      0.0

&end
